    <!-- main section -->
    <div class="container">
        <div class="row d-flex justify-content-center align-items-center mt-5">
            <div class="col-sm-12">
                <% for(let user of users) { %>
                <ul class="list-group">
                    <li class="list-group-item"><a href="/user/<%= user._id %>"><%= user.firstName %>
                            <%= user.lastName %></a></li>
                </ul>
                <% } %>
            </div>
        </div>
    </div>
